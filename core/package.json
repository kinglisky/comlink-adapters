{
    "name": "comlink-adapters",
    "private": false,
    "version": "0.0.2",
    "description": "Implementation of comlink adapters for different application platforms",
    "keywords": [
        "comlink",
        "adapters",
        "message passing",
        "electron",
        "figma",
        "chrome extensions",
        "socket.io",
        "proxy",
        "MessagePort",
        "MessageChannel"
    ],
    "type": "module",
    "files": [
        "dist",
        "README.md"
    ],
    "main": "./dist/comlink-adapters.umd.cjs",
    "module": "./dist/comlink-adapters.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/comlink-adapters.js",
            "require": "./dist/comlink-adapters.umd.cjs"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "tsc && vite build --watch",
        "build": "tsc && vite build",
        "pub": "pnpm publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kinglisky/comlink-adapters.git"
    },
    "author": {
        "name": "kinglisky",
        "email": "kinglisky@icloud.com"
    },
    "devDependencies": {
        "@figma/plugin-typings": "^1.63.0",
        "@types/chrome": "^0.0.237",
        "comlink": "^4.4.1",
        "electron": "^23.1.1",
        "socket.io": "^4.7.2",
        "socket.io-client": "^4.7.2",
        "typescript": "^5.0.2",
        "vite": "^4.3.2",
        "vite-plugin-dts": "^2.3.0"
    }
